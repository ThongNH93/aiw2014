<script>
    $(window).load(function () {
        $().UItoTop({
            easingType: 'easeOutQuart'
        });
    });
    $(function () {
        $('.gallery a.gal').touchTouch();
    });
</script>
<div class="main">
  <div class="content">
    <div class="container_12" >
      <div class="grid_12">
        <h3>Our Gallery</h3>
      </div>
      <div class="gallery">
        <% @images_galleries.each_with_index do |image,i| %>
            <% if i%3==0 %>
                <div class="clear"></div>
            <% end %>
            <div class="grid_4">
              <%= link_to image.image_url, :class =>"gal img_inner" do %>
                 <img src=<%= image_path(image.image) %> alt="" height="194px" width="360px">
              <%end%>
            </div>
        <%end%>
      </div>
    </div>
  </div>
</div>