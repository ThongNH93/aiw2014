<script>
    $(window).load(function () {
        $().UItoTop({
            easingType: 'easeOutQuart'
        });
    });
    $(function () {
        $('.gallery a.gal').touchTouch();
    });
</script>
<div class="main">
  <div class="content">
    <div class="container_12" style="height: auto">
      <div class="grid_12">
        <h3>Our Gallery</h3>
      </div>
      <div class="gallery">
        <% @images_galleries.each_with_index do |image,i| %>
            <% if i%3==0 %>
                <div class="clear"></div>
            <% end %>
            <div class="grid_4">
              <%= link_to home_image_path(image.id), :class =>"gal img_inner" do %>
                 <img src=<%= image_path(image.image) %> alt="">
              <%end%>
            </div>
        <%end%>
      </div>
    </div>
  </div>
  <div class="bottom_block">
    <div class="container_12">
      <div class="grid_2 prefix_2">
        <ul>
          <li><a href="#">FAQS Page</a></li>
        </ul>
      </div>
      <div class="grid_2">
        <ul>
          <li><a href="#">Partners</a></li>
        </ul>
      </div>
      <div class="grid_2">
        <ul>
          <li><a href="#">Useful link</a></li>
        </ul>
      </div>
      <div class="grid_2">
        <h4>Contact Us:</h4>
        TEL: 04644321<br>
        <a href="#">info@vietnambeauty.org</a> </div>
      <div class="clear"></div>
    </div>
  </div>
</div>